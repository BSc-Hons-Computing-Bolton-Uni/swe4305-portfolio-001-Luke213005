package Logbook.Week4;

public class Main {
    public static void main(String[] args) {
        // Create a Course object with a name and code
        Course course1 = new Course("CS101", "Computer Science");

        // Create a Student object with ID and Name (marks are assigned randomly in the constructor)
        Student student1 = new Student(100182377, "Luke");

        // Enroll student in the course
        student1.enrol(course1);

        // Print student details, course details, and marks
        student1.print();
    }
}
