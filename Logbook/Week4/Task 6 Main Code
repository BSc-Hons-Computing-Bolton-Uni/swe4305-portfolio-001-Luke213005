package Logbook.Week4;

public class Main {
    public static void main(String[] args) {
        // Create a Course object with a name and code
        Course course1 = new Course("BOLTON Computing\n" +
                "Level 4 (A1007-1001)", "Computer Science");

        // Create a Student object with ID, Name, and Marks
        Student student1 = new Student(100182377, "Luke", 85, 90, 78, 92);  // Marks are passed to the constructor

        // Enroll student in a course
        student1.enrol(course1);

        // Print student details, course details, and marks
        student1.print();
    }
}
