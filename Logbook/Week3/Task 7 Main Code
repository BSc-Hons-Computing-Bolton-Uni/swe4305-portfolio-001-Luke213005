package Logbook.Week3; // Ensure this matches the package declaration

public class Main {
    public static void main(String[] args) {
        // Create a new Course object with a course code and name passed to the constructor
        Course course1 = new Course("SWE4305", "Computing - Object Oriented Programming");

        // Print the course details using the print method
        System.out.println("Testing course details...");
        course1.print(); // This will display the course code and name
    }
}
