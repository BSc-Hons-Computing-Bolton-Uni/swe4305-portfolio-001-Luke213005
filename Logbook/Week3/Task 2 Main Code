package Logbook.Week3; // Declaring the same package as Student.java

public class Main {
    public static void main(String[] args) {
        // Create a new Student object with ID 101 and Name "Alice"
        Student student1 = new Student(101, "Alice");

        // Display the student's details
        student1.displayStudent();

        // Modify student information using setters
        student1.setId(102);
        student1.setName("Bob");

        // Display updated student details
        student1.displayStudent();
    }
}
