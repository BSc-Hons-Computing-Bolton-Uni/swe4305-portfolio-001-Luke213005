package Logbook.Week2;

public class Main {
    public static void main(String[] args) {
        int number = 7; // Multiplication table for 7

        # will do the times like it is told to do
        for (int i = 1; i <= 12; i++) {
            System.out.println(i + " x " + number + " = " + (i * number));
        }
    }
}
